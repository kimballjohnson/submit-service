{
  "name": "submit-service",
  "version": "1.0.0",
  "description": "Standalone service that samples data sources and creates pull requests",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "node test/test | tap-dot",
    "travis": "npm test",
    "lint": "eslint .",
    "validate": "npm ls",
    "deploy": "claudia update --profile claudia --handler lambda.handler --deploy-proxy-api --region us-east-1"
  },
  "keywords": [],
  "author": "trescube",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.182.0",
    "aws-serverless-express": "^3.0.2",
    "body-parser": "^1.18.2",
    "csv-parse": "^2.0.0",
    "dbfstream": "git://github.com/trescube/dbfstream#add-error-handling",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "github": "^13.1.0",
    "helmet": "^3.9.0",
    "indian-ocean": "^3.0.2",
    "jsftp": "^2.1.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "oboe": "^2.1.4",
    "request": "^2.83.0",
    "sha1": "^1.1.1",
    "stream-to-string": "^1.1.0",
    "string-to-stream": "^1.1.0",
    "temp": "^0.8.3",
    "through2": "^2.0.3",
    "unzip-stream": "^0.3.0",
    "url-join": "^4.0.0",
    "winston": "^3.0.0-rc1",
    "yauzl": "^2.9.1"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "claudia": "^3.2.0",
    "eslint": "^4.15.0",
    "express-list-endpoints": "^3.0.1",
    "ftp-srv": "^2.15.0",
    "get-port": "^3.2.0",
    "precommit-hook": "^3.0.0",
    "proxyquire": "^1.8.0",
    "request-promise": "^4.2.2",
    "tap-dot": "^1.0.1",
    "tape": "^4.2.2"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "curly": "error",
      "eqeqeq": "error",
      "max-depth": [
        "error",
        4
      ],
      "max-len": [
        "error",
        140
      ],
      "max-params": [
        "error",
        4
      ],
      "newIsCap": true,
      "no-global-assign": "error",
      "no-empty": "error",
      "no-unreachable": "error",
      "no-new": "error",
      "no-plusplus": "error",
      "no-undef": "error",
      "no-unused-expressions": "error",
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "public/jquery*"
  ]
}
